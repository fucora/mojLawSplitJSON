{
	"法規性質": "命令",
	"法規名稱": "警察刑事紀錄證明收費標準",
	"法規網址": "https://law.moj.gov.tw/LawClass/LawOldVer.aspx?PCODE=D0080159&LNNDATE=20071026&LSER=001",
	"法規類別": "內政法規＞警政目",
	"異動日期": "20071026",
	"法規內容": [
		{
			"條號": "第 1 條",
			"條文內容": "本標準依警察刑事紀錄證明核發條例第九條規定及規費法第十條第一項規\r\n定訂定之。"
		},
		{
			"條號": "第 2 條",
			"條文內容": "申請核發警察刑事紀錄證明者，應繳納證明書費用每份新臺幣二百五十元\r\n。"
		},
		{
			"條號": "第 3 條",
			"條文內容": "本標準自發布日施行。"
		}
	]
}