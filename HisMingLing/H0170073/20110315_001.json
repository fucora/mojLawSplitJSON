{
	"法規性質": "命令",
	"法規名稱": "嘉義文化創意產業園區場地使用收費標準",
	"法規網址": "http://law.moj.gov.tw/LawClass/LawOldVer.aspx?PCODE=H0170073&LNNDATE=20110315&LSER=001",
	"法規類別": "行政＞文化部＞建設目",
	"異動日期": "20110315",
	"法規內容": [
		{
			"條號": "第 1 條",
			"條文內容": "本標準依規費法第十條第一項規定訂定之。"
		},
		{
			"條號": "第 2 條",
			"條文內容": "使用本園區場地，應依本標準規定繳納使用規費，其收費基準如附表。"
		},
		{
			"條號": "第 3 條",
			"條文內容": "本園區場地開放所收取之場地使用規費將依法全數繳入國庫。"
		},
		{
			"條號": "第 4 條",
			"條文內容": "使用本園區場地應遵守本園區場地設施申請及管理相關規定。"
		},
		{
			"條號": "第 5 條",
			"條文內容": "本標準自發布日施行。"
		}
	]
}